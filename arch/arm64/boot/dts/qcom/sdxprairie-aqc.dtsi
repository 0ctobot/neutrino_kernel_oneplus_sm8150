&pcie0_rp {
	aqc_x4: aquantia,aqc107@pcie0_rp {
		reg = <0 0 0 0 0>;

		compatible = "aquantia,aqc-107";

		pci-ids = "1d6a:d107";

		qcom,smmu;
		qcom,smmu-iova-base = /bits/ 64 <0x0>;
		qcom,smmu-iova-size = /bits/ 64 <0x80000000>;

		qcom,smmu-attr-s1-bypass;
	};
};

